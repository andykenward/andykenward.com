{
  "name": "andykenward.com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "^10.17.0"
  },
  "scripts": {
    "dev": "next",
    "dev:now": "now dev",
    "prebuild": "npm run codegen",
    "build": "next build",
    "postbuild": "npm run export",
    "export": "next export",
    "start": "next start",
    "now-build": "npm run build",
    "now-dev": "npm run dev",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml",
    "codegen:watch": "npm run codegen -- --watch",
    "format": "prettier --write '**/*.{css,js,jsx,ts,tsx}'",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "eslint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:ci": "jest --coverage",
    "schema": "apollo service:download --config=./apollo.config.prod.js generated/schema.json",
    "normalizecss": "curl https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css  -o ./public/static/normalize.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andykenward/andykenward.com.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andykenward/andykenward.com/issues"
  },
  "homepage": "https://github.com/andykenward/andykenward.com#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run eslint:fix",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@apollo/react-ssr": "3.1.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@styled-system/should-forward-prop": "5.1.4",
    "apollo-boost": "0.4.7",
    "emotion-theming": "10.0.27",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.1.7",
    "normalize.css": "8.0.1",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-system": "5.1.4"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.3",
    "@graphql-codegen/cli": "1.11.2",
    "@graphql-codegen/fragment-matcher": "1.11.2",
    "@graphql-codegen/introspection": "1.11.2",
    "@graphql-codegen/typescript": "1.11.2",
    "@graphql-codegen/typescript-operations": "1.11.2",
    "@graphql-codegen/typescript-react-apollo": "1.11.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.0.25",
    "@types/node": "10.14.15",
    "@types/ramda": "0.26.36",
    "@types/react": "16.9.18",
    "@types/react-dom": "16.9.5",
    "@types/styled-system": "5.1.4",
    "@types/testing-library__react": "9.1.2",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "apollo": "2.21.3",
    "babel-jest": "24.9.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "cross-env": "^6.0.3",
    "csstype": "2.6.8",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "husky": "4.0.10",
    "jest": "24.9.0",
    "jest-emotion": "10.0.27",
    "lint-staged": "10.0.0-1",
    "now": "16.7.3",
    "prettier": "1.19.1",
    "typescript": "3.7.5"
  }
}

    "@graphql-codegen/typescript-operations": "1.17.12",
    "graphql": "15.4.0",
    "graphql-request": "3.4.0",
    "graphql-tag": "2.11.0",
    "next": "10.0.3",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "theme-ui": "0.3.4",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/node": "^12.19.13",
    "@types/ramda": "0.27.34",

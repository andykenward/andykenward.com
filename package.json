  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "next dev",
    "dev:vercel": "vercel dev",
    "prebuild": "npm run codegen",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml",
    "codegen:watch": "npm run codegen -- --watch",
    "format": "prettier --write '**/*.{css,js,jsx,ts,tsx,graphql}'",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "eslint:fix": "npm run lint -- --fix",
    "schema": "npx apollo service:download --config=./apollo.config.prod.js generated/schema.json",
    "local:ci": "npm run codegen && npm run lint && npm run type-check && npm run test:ci",
    "test": "jest",
    "test:ci": "npm test -- --ci",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andykenward/andykenward.com.git"
  },
